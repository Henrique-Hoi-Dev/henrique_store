# Configurações do Mercado Pago
MERCADO_PAGO_ACCESS_TOKEN=APP_USR-xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
MERCADO_PAGO_WEBHOOK_URL=https://your-domain.com/api/webhook

# Configurações da API
API_BASE_URL=https://your-domain.com

# Configurações do Ambiente
NODE_ENV=production
PORT=3000

# Configurações de Admin (opcional)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=secure_password

# Configurações de Fornecedor (opcional)
SUPPLIER_API_URL=https://api.supplier.com
SUPPLIER_API_KEY=your_api_key

# Configurações de Banco de Dados (se necessário)
DATABASE_URL=your_database_url

# Configurações de Email (se necessário)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_email_password

# Configurações de Analytics (opcional)
GOOGLE_ANALYTICS_ID=GA_MEASUREMENT_ID
FACEBOOK_PIXEL_ID=your_pixel_id

# Configurações de Segurança
SESSION_SECRET=your_session_secret_key
JWT_SECRET=your_jwt_secret_key

# Configurações de Cache (opcional)
REDIS_URL=redis://localhost:6379

# Configurações de Upload (opcional)
CLOUDINARY_URL=cloudinary://your_cloudinary_url
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
AWS_REGION=us-east-1
AWS_S3_BUCKET=your_s3_bucket_name 